#include <lib.h>
#include "../customdefs.h";
inherit  LIB_SENTIENT;
void create(){
    ::create();
  SetKeyName("dude");
  SetId("guy");
  SetShort("The dude.");
  SetLong("This is The Dude.");
  SetInventory(([  MY_OBJ + "/" +"key" : 1]));
}
//funs
//snuf
void init() {
::init();
  SetSmell(([  "default" : "A rather odd smell."]));
  SetListen(([  "default" : "Nobody listens to the dude."]));
}
//extras
